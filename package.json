    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "express-session": "^1.15.3",
    "hasha": "^3.0.0",
    "jwks-rsa": "^1.2.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.1",
    "mongoose-unique-validator": "^1.0.5",
